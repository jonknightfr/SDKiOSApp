# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'SDK App' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iOS-base
  pod 'FRAuth'
  pod 'FRAuthenticator'
  pod 'FRUI'
  pod 'FRProximity'
  pod 'FRFacebookSignIn'
  pod 'FRDeviceBinding'
  pod 'PingOneSignals', :podspec => "https://assets.pingone.com/signals/ios-sdk/5.2.1/PingOneSignals.podspec"
  
  pod 'SwiftyJSON'
end
